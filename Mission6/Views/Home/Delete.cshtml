@{
    ViewData["Title"] = "Delete";
}

@model MovieForm


<h2>Confirm Deletion</h2>
<br />
<h3>Are you sure you want to delete the record for @Model.Title?</h3>
<br />

<form method="post" asp-controller="Home" asp-action="Delete">

    <button type="submit">Delete</button>
    <a asp-controller="Home" asp-action="SeeMovies" class="btn btn-primary">Cancel</a>

    <input type="hidden" asp-for="AppId"/>
</form>
